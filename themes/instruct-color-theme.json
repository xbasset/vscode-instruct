{
    "name": "PT Color Theme",
    "type": "dark",
    "colors": {
      "editor.foreground": "#FFFFFF",
      "editor.background": "#1E1E1E"
    },
    "tokenColors": [
      {
        "scope": "comment.line.shebang.green.instruct",
        "settings": {
          "foreground": "#00FF00",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "comment.line.instruct",
        "settings": {
          "foreground": "#888888",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "bracket.round.instruct",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "bracket.square.instruct",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "bracket.curly.instruct",
        "settings": {
          "foreground": "#ff0000"
        }
      },
      {
        "scope": "punctuation.other.jinja",
        "settings": {
          "foreground": "#f97272"
        }
      },
      {
        "scope": "keyword.control.jinja",
        "settings": {
          "foreground": "#c27bc3"
