      },
      {
        "scope": "comment.block.jinja",
        "settings": {
          "foreground": "#00a413"
        }
      },
      {
        "scope": "variable.other.jinja.filter",
        "settings": {
          "foreground": "#a9c605"
        }
      },
      {
        "scope": "variable.other.jinja",
        "settings": {
          "foreground": "#8ec7ed"
        }
      },
      {
        "scope": "entity.name.tag.instruct",
        "settings": {
          "foreground": "#00a2ff"
        }
      },
      {
        "scope": "punctuation.definition.tag.instruct",
        "settings": {
          "foreground": "#00a2ff"
        }
      }
    ]
  }
  